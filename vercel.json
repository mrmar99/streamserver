{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./index.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/files/(.*)",
      "dest": "/files/$1"
    },
    {
      "src": "/",
      "dest": "index.js"
    }
  ]
}